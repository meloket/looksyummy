{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "benwilkins/laravel-fcm-notification": "dev-master",
        "brozot/laravel-fcm": "^1.2",
        "guzzle/guzzle": "~3.9",
        "guzzlehttp/guzzle": "^6.5",
        "iivannov/larasquare": "^0.1.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "~5.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^2.1",
        "orangehill/iseed": "dev-master",
        "rap2hpoutre/laravel-log-viewer": "^0.14.3",
        "ravaelles/filterable": "^2.1",
        "skagarwal/google-places-api": "^1.1",
        "square/connect": "^2.20190410"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "xethron/migrations-generator": "^2.0",
		"filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
		"files": [
            "app/Helpers/string.php",
			"vendor/connect-php-sdk/autoload.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
